<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.adonai.ui.editor2.AddPartController" alignment="CENTER" hgap="10" vgap="10">
    <padding><Insets top="40" bottom="40" left="30" right="30"/> </padding>

    <Label fx:id="lblTitle" text="ADD NEW PART" id="header" GridPane.halignment="CENTER" GridPane.fillWidth ="true" GridPane.rowIndex="0"/>

    <TextField fx:id="txtSearch" GridPane.columnIndex="0" GridPane.rowIndex="1" GridPane.fillHeight="true" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"/>
    <ListView fx:id="lviTypes" GridPane.columnIndex="0" GridPane.rowIndex="2" GridPane.fillHeight="true" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"/>

</GridPane>